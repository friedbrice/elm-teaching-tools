{
    "type": "package",
    "name": "friedbrice/elm-teaching-tools",
    "summary": "Tools for using Elm in the classroom.",
    "license": "BSD-3-Clause",
    "version": "1.0.1",
    "exposed-modules": [
        "ElmTeachingTools.Labs.Introduction",
        "ElmTeachingTools.Labs.Life",
        "ElmTeachingTools.Lib.Color",
        "ElmTeachingTools.Lib.Game",
        "ElmTeachingTools.Lib.Graphics",
        "ElmTeachingTools.Lib.Keyboard",
        "ElmTeachingTools.Lib.List",
        "ElmTeachingTools.Lib.Maybe",
        "ElmTeachingTools.Lib.Memoize",
        "ElmTeachingTools.Lib.String"
